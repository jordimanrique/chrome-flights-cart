{
  "manifest_version": 2,
  "name": "Atrapalo Cart",
  "description": "Atrapalo Cart",
  "version": "0.0.1",
  "icons": {
    "128" : "icons/airplane_128.png"
  },
  "page_action": {
    "default_icon": "icons/airplane_128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/carrito/procesar_compra/*"],
      "js": ["js/lib/jquery.js", "js/lib/jquery.colorbox-min.js", "js/contentScript.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ]
}
